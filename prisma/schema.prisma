generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Albums {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  title  String
  userId String @db.ObjectId
}

model Photos {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  albumId   String @db.ObjectId
  caption   String
  contrast  Float
  exposure  Float
  highlight Float
  shadow    Float
  url       String
}

model Users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  name     String
  password String
}
